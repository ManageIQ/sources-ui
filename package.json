    "fetch-mock": "^7.7.3",
    "file-loader": "^1.1.11",
    "git-revision-webpack-plugin": "^3.0.4",
    "glob": "^7.1.6",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "koa-connect": "^2.0.1",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.22.1",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.3",
    "sass-loader": "^7.3.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
