    "@patternfly/react-core": "^3.124.1",
    "@patternfly/react-icons": "^3.14.28",
    "@patternfly/react-table": "^2.24.64",
    "@redhat-cloud-services/frontend-components": "^0.0.43",
    "@redhat-cloud-services/frontend-components-notifications": "^0.0.7",
    "@redhat-cloud-services/frontend-components-sources": "^0.0.27",
    "@redhat-cloud-services/frontend-components-utilities": "^0.0.15",
    "@redhat-cloud-services/sources-client": "^1.0.45",
    "react": "^16.12.0",
    "react-content-loader": "^3.4.2",
    "react-dom": "^16.12.0",
    "react-intl": "^3.9.2",
